{
  "name": "@inshel/react-examples",
  "version": "0.1.0",
  "description": "InShel examples",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "develop": "webpack-dev-server --hot",
    "develop:server": "babel-node ./src/server.js",
    "compile:client": "webpack --config webpack.config.js",
    "compile:server": "webpack --config webpack.server.config.js",
    "compile": "rm -rf ./target && npm run compile:client && npm run compile:server",
    "docker:image:node": "docker build -f docker.node -t inshel-examples-node:$npm_package_version .",
    "docker:image:static": "docker build -f docker.static -t inshel-examples-static:$npm_package_version .",
    "docker:images": "npm run compile && npm run docker:image:node && npm run docker:image:static",
    "docker:publish:node": "",
    "docker:publish:static": "",
    "docker:publish": "npm run docker:images && npm run docker:publish:node && npm run docker:publish:static"
  },
  "keywords": [
    "InShel",
    "examples"
  ],
  "author": "Dmitry <db@inshel.com> Balakov",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
